function exampleLoop() -> i32 {
    i : i32 = 0;
    j : i32 = 0;
    for(i = 0; i< 10; i=i+1){
        for(j = 0; j < i; j=j+1){
            print( 'i + j is \ '+(!str!(i+j)));
        }
        print('_______________');
    }
    return 1;
}

function main () -> i32 {
exampleLoop();
return 1;
}
main();